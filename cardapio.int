<!DOCTYPE html>
<html lang="pt-BR">
<head>
  <meta charset="UTF-8">
  <title>Padaria P√£o & Sabor | Autoatendimento</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <!-- Tailwind -->
  <script src="https://cdn.tailwindcss.com"></script>
</head>

<body class="bg-gray-100">

<!-- HEADER -->
<header class="bg-yellow-500 text-white p-8 text-center font-bold text-xl">
  ü•ñ Padaria P√£o & Sabor
</header>

<!-- CAT√ÅLOGO -->
<main class="p-4 pb-24">
  <h2 class="text-2xl font-bold mb-4">Fa√ßa seu pedido</h2>

  <div class="grid grid-cols-1 sm:grid-cols-2 gap-4">

    <!-- PRODUTO -->
    <div class="bg-white rounded-xl shadow p-4">
      <img src="data:image/jpeg;base64,"
        class="h-36 w-full object-cover rounded-lg mb-2">
      <h3 class="font-bold">P√£o Franc√™s</h3>
      <p class="text-sm text-gray-500">Unidade</p>
      <p class="font-bold text-lg">R$ 0,80</p>
      <button onclick="adicionar('P√£o Franc√™s', 0.8)"
        class="w-full bg-yellow-500 text-white py-2 rounded-lg mt-2">
        Adicionar
      </button>
    </div>

    <div class="bg-white rounded-xl shadow p-4">
      <img src="https://sabores-new.s3.amazonaws.com/public/2025/01/COXINHA-scaled.jpg"
        class="h-36 w-full object-cover rounded-lg mb-2">
      <h3 class="font-bold">Coxinha</h3>
      <p class="text-sm text-gray-500">Frango com catupiry</p>
      <p class="font-bold text-lg">R$ 5,00</p>
      <button onclick="adicionar('Coxinha', 5)"
        class="w-full bg-yellow-500 text-white py-2 rounded-lg mt-2">
        Adicionar
      </button>
    </div>

    <div class="bg-white rounded-xl shadow p-4">
        <img src="https://www.zappas.com.br/image/cache/catalog/misto%20quente-800x800.png"
          class="h-36 w-full object-cover rounded-lg mb-2">
        <h3 class="font-bold">Misto quente</h3>
        <p class="text-sm text-gray-500">Presunto e Queijo</p>
        <p class="font-bold text-lg">R$ 7,00</p>
        <button onclick="adicionar('Misto quente', 7)"
          class="w-full bg-yellow-500 text-white py-2 rounded-lg mt-2">
          Adicionar
        </button>
      </div>

    <div class="bg-white rounded-xl shadow p-4">
      <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTiqTgTh_tOXpE5YnVcGe0D6dWAeYSZjvsDq_bj7hHqug&s"
        class="h-36 w-full object-cover rounded-lg mb-2">
      <h3 class="font-bold">Caf√©</h3>
      <p class="text-sm text-gray-500">200ml</p>
      <p class="font-bold text-lg">R$ 3,00</p>
      <button onclick="adicionar('Caf√©', 3)"
        class="w-full bg-yellow-500 text-white py-2 rounded-lg mt-2">
        Adicionar
      </button>
    </div>

    <div class="bg-white rounded-xl shadow p-4">
        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSyJwGcmHL9XsQ-4SyQKKJfTvaKfivGjYJpmg&s"
          class="h-36 w-full object-cover rounded-lg mb-2">
        <h3 class="font-bold">Sonho</h3>
        <p class="text-sm text-gray-500">Recheio de creme</p>
        <p class="font-bold text-lg">R$ 2,50</p>
        <button onclick="adicionar('Sonho', 2.5)"
          class="w-full bg-yellow-500 text-white py-2 rounded-lg mt-2">
          Adicionar
        </button>
      </div>

      <div class="bg-white rounded-xl shadow p-4">
        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTWzE1b1V342NuFhfgRxnpuzYLRpb8_G2-4sw&s"
          class="h-36 w-full object-cover rounded-lg mb-2">
        <h3 class="font-bold">Suco natural</h3>
        <p class="text-sm text-gray-500">500 ml - sabor Laranja</p>
        <p class="font-bold text-lg">R$ 8,50</p>
        <button onclick="adicionar('suco', 8.5)"
          class="w-full bg-yellow-500 text-white py-2 rounded-lg mt-2">
          Adicionar
        </button>
      </div>

  </div>
</main>

<!-- CARRINHO TELA CHEIA (MOBILE) -->
<aside id="carrinho"
  class="fixed inset-0 bg-white z-50 p-4 hidden overflow-y-auto">

  <div class="flex justify-between items-center mb-4">
    <h2 class="text-xl font-bold">üõí Seu Pedido</h2>
    <button onclick="toggleCarrinho()" class="text-red-500 text-xl">‚úñ</button>
  </div>

  <ul id="listaCarrinho" class="space-y-2 mb-4"></ul>

  <div class="border-t pt-4">
    <p class="font-bold text-lg mb-2">
      Total: R$ <span id="total">0.00</span>
    </p>
    <button onclick="finalizarPedido()"
      class="w-full bg-green-500 text-white py-3 rounded-lg text-lg">
      Finalizar Pedido
    </button>
  </div>
</aside>

<!-- BARRA FIXA MOBILE -->
<div class="fixed bottom-0 left-0 right-0 bg-white border-t shadow-lg p-3 flex justify-between items-center">
  <span class="font-bold">
    üõí <span id="qtdCarrinhoMobile">0</span> itens
  </span>

  <button onclick="toggleCarrinho()"
    class="bg-yellow-500 text-white px-5 py-2 rounded-lg font-semibold">
    Ver Carrinho ‚Ä¢ R$ <span id="totalMobile">0.00</span>
  </button>
</div>

<!-- MODAL PAGAMENTO -->
<div id="modalPagamento"
  class="fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center hidden z-50">

  <div class="bg-white rounded-xl p-6 w-11/12 max-w-sm">
    <h2 class="text-2xl font-bold mb-4 text-center">üí≥ Pagamento</h2>

    <button onclick="pagarPIX()"
      class="w-full bg-green-500 text-white py-3 rounded-lg mb-3">
      üì≤ PIX
    </button>

    <button onclick="pagarCartao()"
      class="w-full bg-blue-500 text-white py-3 rounded-lg mb-3">
      üí≥ Cart√£o
    </button>

    <button onclick="fecharModal()"
      class="w-full bg-gray-300 py-2 rounded-lg">
      Cancelar
    </button>
  </div>
</div>

<!-- SCRIPT -->
<script>
let carrinho = [];
let total = 0;

function adicionar(nome, preco) {
  carrinho.push({ nome, preco });
  total += preco;
  atualizarCarrinho();
}

function atualizarCarrinho() {
  const lista = document.getElementById('listaCarrinho');
  lista.innerHTML = '';

  carrinho.forEach(item => {
    const li = document.createElement('li');
    li.className = 'flex justify-between';
    li.innerHTML = `<span>${item.nome}</span><span>R$ ${item.preco.toFixed(2)}</span>`;
    lista.appendChild(li);
  });

  document.getElementById('total').innerText = total.toFixed(2);
  document.getElementById('qtdCarrinhoMobile').innerText = carrinho.length;
  document.getElementById('totalMobile').innerText = total.toFixed(2);
}

function toggleCarrinho() {
  document.getElementById('carrinho').classList.toggle('hidden');
}

function finalizarPedido() {
  if (carrinho.length === 0) {
    alert("Carrinho vazio!");
    return;
  }
  document.getElementById('modalPagamento').classList.remove('hidden');
}

function fecharModal() {
  document.getElementById('modalPagamento').classList.add('hidden');
}

function pagarPIX() {
  fecharModal();
  alert(
    `üì≤ PIX GERADO\n\nChave: padaria@paoesabor.com\nValor: R$ ${total.toFixed(2)}\n\n(Aguardando pagamento...)`
  );
  confirmarPagamento("PIX");
}

function pagarCartao() {
  fecharModal();
  const tipo = prompt("Digite: cr√©dito ou d√©bito");
  if (!tipo) return;
  alert(`üí≥ Pagamento no ${tipo.toUpperCase()} aprovado!`);
  confirmarPagamento("Cart√£o");
}

function confirmarPagamento(tipo) {
  alert(`‚úÖ Pagamento via ${tipo} confirmado!\nPedido enviado para a cozinha ü•ê`);
  carrinho = [];
  total = 0;
  atualizarCarrinho();
  toggleCarrinho();
}
</script>

</body>
</html>
